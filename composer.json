{
  "name": "broadway/saga-state-dbal",
  "description": "Saga state persistence for broadway/broadway-saga using doctrine/dbal",
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "broadway/broadway-saga": "^1.0.0",
    "doctrine/dbal": "^3.1",
    "beberlei/assert": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "ramsey/uuid": "^4.0",
    "broadway/coding-standard": "^1.2",
    "phpstan/phpstan": "@stable"
  },
  "authors": [
    {
      "name": "othillo",
      "email": "othillo@othillo.nl"
    }
  ],
  "autoload": {
    "psr-4": {
      "Broadway\\Saga\\State\\Dbal\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Broadway\\Saga\\State\\Dbal\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
